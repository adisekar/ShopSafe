
@{
    ViewBag.Title = "ShopSafe";
}

<h1>Selected store -  @ViewBag.Store at @ViewBag.Location</h1>

<div id="root"></div>

@section scripts{
    <script type="text/javascript">
        window.MAX_CAPACITY = @ViewBag.MaxCapacity;
        window.INITIAL_EXISTING_CAPACITY = @ViewBag.CurrentCapacity;
        window.ADMIN = false;

    </script>
    <script src="~/Scripts/bundle.js"></script>
}